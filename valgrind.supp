{
   <readline>
   Memcheck:Leak
   match-leak-kinds: definite
   fun:malloc
   fun:xmalloc
   fun:readline_internal_teardown
   fun:readline
   fun:main
}
{
   <history>
   Memcheck:Leak
   match-leak-kinds: reachable
   fun:malloc
   fun:xmalloc
   fun:add_history
   fun:main
}
{
   <ls>
   Memcheck:Leak
   match-leak-kinds: reachable
   ...
   obj:/usr/bin/ls
   ...
   fun:(below main)
}
